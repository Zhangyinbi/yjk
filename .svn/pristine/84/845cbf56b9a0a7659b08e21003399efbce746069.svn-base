<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/gray_dcdbdb"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="@color/red_c30d22"
        android:gravity="center_vertical">

        <RelativeLayout
            android:id="@+id/left_igame_back_relative"
            android:layout_width="48dp"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:background="@drawable/selector_title_btn_bg"
            android:gravity="center">

            <ImageView
                android:id="@+id/left_image"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_centerInParent="true"
                android:src="@drawable/back" />
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/linear_titleTv"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:layout_centerVertical="true"
            android:background="@drawable/selector_title_btn_bg"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <com.xiaomizhuang.buildcaptain.view.MarqueeTextView
                android:id="@+id/tv_orders_title"
                android:layout_width="@dimen/x350"
                android:layout_height="wrap_content"
                android:ellipsize="marquee"
                android:gravity="center"
                android:marqueeRepeatLimit="marquee_forever"
                android:singleLine="true"
                android:text="@string/str_orders_detail"
                android:textColor="@color/white_style_ffffff"
                android:textSize="@dimen/textsize_40px" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/pulldown"
                android:visibility="gone" />

        </LinearLayout>


        <TextView
            android:id="@+id/right_text0_xiadan"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_alignWithParentIfMissing="true"
            android:layout_centerVertical="true"
            android:background="@drawable/selector_title_btn_bg"
            android:clickable="true"
            android:gravity="center"
            android:paddingLeft="@dimen/x20"
            android:paddingRight="@dimen/x20"
            android:text="@string/str_orders"
            android:textColor="@color/white_style_ffffff"
            android:textSize="@dimen/textsize_30px" />
    </RelativeLayout>


    <GridView
        android:id="@+id/gv_assist_type"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:listSelector="@color/transparent"
        android:numColumns="3"
        android:padding="@dimen/x10"
        tools:listitem="@layout/assist_type_item">

    </GridView>

