<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/ll_report_item"
              xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/white"
              android:orientation="vertical">

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="#e2e2e2"
        android:gravity="center_vertical"
        android:paddingLeft="12dp"
        android:text="@string/receiving_reprot"
        android:textColor="@color/text_959595"
        android:textSize="14sp"/>

    <TextView
        android:id="@+id/tv_super_upload_time"
        style="@style/Fm_ffffff_36_Tv"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center_vertical"
        android:paddingLeft="12dp"
        android:text="最新上传时间：2016.06.24"
        android:textSize="15sp"/>

    <View
        style="@style/view_grayd1d1d1_divider"
        android:layout_marginLeft="@dimen/x24"
        android:layout_marginRight="@dimen/x24"/>

    <ImageView
        android:id="@+id/imgv_super_item"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="10dp"
        android:scaleType="fitXY"/>

    <TextView
        android:id="@+id/tv_no_picture"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:gravity="center"
        android:layout_gravity="center_horizontal"
        android:text="暂无图片"
        android:textSize="14sp"
        android:textColor="@color/text_959595"
        android:visibility="gone"
        />

</LinearLayout>
